<% provide(:title, "Quota Associativa: #{@association_fee.name}" ) %>

<%= form_for(@association_fee, :html => { :class => "form-horizontal" }) do |f| %>
  <%= render 'shared/error_messages' %>

  <h1><%= @association_fee.name %></h1>

  <div class="row">
  <div class="span6 offset3">

    <%= f.label :valid_year %>
    <%= f.text_field :valid_year, :disabled => 'disabled' %>

    <%= f.label :value %>
    <%= f.text_field :value, :disabled => 'disabled' %>

    <%= f.label :is_total %>
    <%= f.check_box :is_total, :disabled => 'disabled' %>

    <%= f.label :active %>
    <%= f.check_box :active, :disabled => 'disabled' %>

    <br/><br/>

    <%= link_to 'Editar', edit_association_fee_path(@association_fee) , class: "btn btn-group-sm btn-info" %>
    <%= link_to 'Voltar AtrÃ¡s', association_fees_path, class: "btn btn-group-sm btn-info" %>
    </div>
  </div>
<% end %>


