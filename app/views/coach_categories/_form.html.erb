<%= form_for(@coach_category) do |f| %>

  <%= render 'layouts/inner_error_messages', {:msg => @coach_category} %>


      <%= f.label :name, "Graduação" %>
      <%= f.text_field :name %>

      <%= f.label :order, "Ordem" %>
      <%= f.text_field :order %>

      <%= f.label :active, "Activo" %>
      <%= f.check_box :active %>

      <br/><br/>

      <%= f.submit "Gravar", class: "btn btn-md btn-success" %>

<% end %>
